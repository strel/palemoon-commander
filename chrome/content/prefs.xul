<prefwindow id="pmcommander-prefs" title="&pmcommander.window.title;"
            xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
            style="" onload="pmcommanderPrefs.init();">

   <script type="application/javascript" src="chrome://pmcommander/content/prefs.js" />
   
   <prefpane id="pmcommander-pane-graphics" label="&pmcommander.pane.graphics;">
   
      <preferences>
         <preference id="pmcommander-pref-gfx-driverbugs"             name="gfx.work-around-driver-bugs"            type="bool" />
         <preference id="pmcommander-pref-initialpaint-delay"         name="nglayout.initialpaint.delay"            type="int" />
         <preference id="pmcommander-pref-webgl-native"               name="webgl.prefer-native-gl"                 type="bool" />
         <preference id="pmcommander-pref-webgl-msaa-force"           name="webgl.msaa-force"                       type="bool" />
         <preference id="pmcommander-pref-webgl-msaa-level"           name="gl.msaa-level"                          type="int" />
         <preference id="pmcommander-pref-d2d-enabled"                name="gfx.direct2d.disabled"                  type="bool" inverted="true" 
           onchange="pmcommanderPrefs.d2dGlobalChanged();" />
         <preference id="pmcommander-pref-d2d-force"                  name="gfx.direct2d.force-enabled"             type="bool" />
         <preference id="pmcommander-pref-layers-enabled"             name="layers.acceleration.disabled"           type="bool" inverted="true" 
           onchange="pmcommanderPrefs.layeraccGlobalChanged();" />
         <preference id="pmcommander-pref-layers-drawfps"             name="layers.acceleration.draw-fps"           type="bool" />
         <preference id="pmcommander-pref-layers-force"               name="layers.acceleration.force-enabled"      type="bool" />
         <preference id="pmcommander-pref-layers-mode"                name="extensions.pmcommander.layers.mode"     type="int" 
           onchange="pmcommanderPrefs.layersModeChanged();" />
         <preference id="pmcommander-pref-rendering-deprecated"       name="layout.altrendering.enabled"            type="bool" inverted="true" />

         <preference id="pmcommander-pref-fonts-directwrite"          name="gfx.font_rendering.directwrite.enabled"                 type="bool" />
         <preference id="pmcommander-pref-fonts-directwrite-gdi"      name="gfx.font_rendering.directwrite.use_gdi_table_loading"   type="bool" />
         <preference id="pmcommander-pref-fonts-graphite"             name="gfx.font_rendering.graphite.enabled"                    type="bool" />
         <preference id="pmcommander-pref-cleartype-content-always"   name="gfx.font_rendering.cleartype.always_use_for_content"    type="bool" />
         <preference id="pmcommander-pref-cleartype-downloadable"     name="gfx.font_rendering.cleartype.use_for_downloadable_fonts" type="bool" />
         <preference id="pmcommander-pref-cleartype-mode"             name="gfx.font_rendering.cleartype_params.rendering_mode"     type="int" />
         <preference id="pmcommander-pref-cleartype-level"            name="gfx.font_rendering.cleartype_params.cleartype_level"    type="int" />
         <preference id="pmcommander-pref-cleartype-contrast"         name="gfx.font_rendering.cleartype_params.enhanced_contrast"  type="int" />
         <preference id="pmcommander-pref-cleartype-gamma"            name="gfx.font_rendering.cleartype_params.gamma"              type="int" />
         <preference id="pmcommander-pref-cleartype-pixel-structure"  name="gfx.font_rendering.cleartype_params.pixel_structure"    type="int" />
         <preference id="pmcommander-pref-cleartype-gdi-max-size"     name="gfx.font_rendering.cleartype_params.force_gdi_classic_max_size" type="int" />
         
         <preference id="pmcommander-pref-color-override-a11y"        name="browser.display.ignore_accessibility_theme" type="bool" />
         <preference id="pmcommander-pref-color-ICCv4"                name="gfx.color_management.enablev4"          type="bool" />
         <preference id="pmcommander-pref-color-ICCmode"              name="gfx.color_management.mode"              type="int" />
         <preference id="pmcommander-pref-color-ICCintent"            name="gfx.color_management.rendering_intent"  type="int" />
      </preferences>

      <tabbox id="pmcommander-tabbox-graphics" flex="1">
         <tabs id="pmcommander-tabs-graphics">
            <tab id="pmcommander-tab-graphics-general" label="&pmcommander.tab.general;" />
            <tab id="pmcommander-tab-graphics-rendering"  label="&pmcommander.tab.graphics.rendering;" />
            <tab id="pmcommander-tab-graphics-3D" label="&pmcommander.tab.graphics.3D;" />
            <tab id="pmcommander-tab-graphics-fonts" label="&pmcommander.tab.graphics.fonts;" />
            <tab id="pmcommander-tab-graphics-color" label="&pmcommander.tab.graphics.color;" />
         </tabs>

         <tabpanels flex="1">
            <tabpanel id="pmcommander-graphics-general" orient="vertical">
                  <checkbox id="pmcommander-gfx-driverbugs-check" preference="pmcommander-pref-gfx-driverbugs" label="&pmcommander.graphics.general.workaround.driverbugs;" />
                  <hbox align="center">
                     <label id="pmcommander-gfx-initialpaint-delay-label" control="pmcommander-gfx-initialpaint-delay-input">&pmcommander.graphics.general.initialpaint.delay.label;</label>
                     <textbox type="number" size="4" min="25" max="3000" id="pmcommander-gfx-initialpaint-delay-input" preference="pmcommander-pref-initialpaint-delay" />
                     <label>&pmcommander.unit.ms;</label>
                  </hbox>
            </tabpanel>
            <tabpanel id="pmcommander-graphics-rendering" orient="vertical">
               <groupbox id="pmcommander-graphics-rendering-d2d">
                  <caption label="&pmcommander.graphics.rendering.d2d.caption;" />
                  <checkbox id="pmcommander-d2d-enabled-check" preference="pmcommander-pref-d2d-enabled" label="&pmcommander.graphics.rendering.d2d.enabled.label;" />
                  <checkbox id="pmcommander-d2d-force-check" preference="pmcommander-pref-d2d-force" label="&pmcommander.graphics.rendering.d2d.force.label;" />
               </groupbox>
               <groupbox id="pmcommander-graphics-rendering-layers">
                  <caption label="&pmcommander.graphics.rendering.layers.caption;" />
                  <checkbox id="pmcommander-layers-enabled-check" preference="pmcommander-pref-layers-enabled" label="&pmcommander.graphics.rendering.layers.enabled.label;" />
                  <checkbox id="pmcommander-layers-force-check" preference="pmcommander-pref-layers-force" label="&pmcommander.graphics.rendering.layers.force.label;" />
                  <hbox align="center">
                     <label id="pmcommander-layers-mode-label" control="pmcommander-layers-mode-menu">&pmcommander.graphics.rendering.layers.mode.label;</label>
                     <menulist id="pmcommander-layers-mode-menu" preference="pmcommander-pref-layers-mode" sizetopopup="always">
                        <menupopup>
                           <menuitem label="&pmcommander.graphics.rendering.layers.mode.default;"    value="0" />
                           <menuitem label="&pmcommander.graphics.rendering.layers.mode.d3d9;" value="1" />
                           <menuitem label="&pmcommander.graphics.rendering.layers.mode.opengl;"  value="2" />
                        </menupopup>
                     </menulist>
                  </hbox>
                  <checkbox id="pmcommander-rendering-deprecated-check" preference="pmcommander-pref-rendering-deprecated" label="&pmcommander.graphics.rendering.use.deprecated.label;" />
               </groupbox>
            </tabpanel>
            <tabpanel id="pmcommander-graphics-3D" orient="vertical">
               <checkbox id="pmcommander-webgl-native-check" preference="pmcommander-pref-webgl-native" label="&pmcommander.graphics.3d.webgl.native.label;" />
               <checkbox id="pmcommander-webgl-msaa-force-check" preference="pmcommander-pref-webgl-msaa-force" label="&pmcommander.graphics.3d.webgl.msaa.force.label;" />
               <hbox align="center">
                  <label id="pmcommander-webgl-msaa-level-label" control="pmcommander-msaa-level-menu">&pmcommander.graphics.3d.webgl.msaa.level.label;</label>
                  <menulist id="pmcommander-layers-mode-menu" preference="pmcommander-pref-webgl-msaa-level" sizetopopup="always">
                     <menupopup>
                        <menuitem label="&pmcommander.graphics.3d.msaa.level.0;"    value="0" />
                        <menuitem label="&pmcommander.graphics.3d.msaa.level.2;"    value="2" />
                        <menuitem label="&pmcommander.graphics.3d.msaa.level.4;"    value="4" />
                        <menuitem label="&pmcommander.graphics.3d.msaa.level.8;"    value="8" />
                        <menuitem label="&pmcommander.graphics.3d.msaa.level.16;"   value="16" />
                     </menupopup>
                  </menulist>
               </hbox>
            </tabpanel>
            <tabpanel id="pmcommander-graphics-fonts" orient="vertical">
               <checkbox id="pmcommander-fonts-directwrite-gdi-check" preference="pmcommander-pref-fonts-directwrite-gdi" label="&pmcommander.graphics.fonts.directwrite.gdi.label;" />
               <checkbox id="pmcommander-fonts-graphite-check" preference="pmcommander-pref-fonts-graphite" label="&pmcommander.graphics.fonts.graphite.label;" />
               <groupbox id="pmcommander-graphics-fonts-cleartype">
                  <caption label="&pmcommander.graphics.fonts.cleartype.caption;" />
                  <checkbox id="pmcommander-cleartype-content-check" preference="pmcommander-pref-cleartype-content-always" label="&pmcommander.graphics.cleartype.content.always.label;" />
                  <checkbox id="pmcommander-cleartype-downloadable-check" preference="pmcommander-pref-cleartype-downloadable" label="&pmcommander.graphics.cleartype.downloadable.label;" />
                  <hbox align="center">
                     <label id="pmcommander-cleartype-mode-label" control="pmcommander-cleartype-mode-menu">&pmcommander.graphics.cleartype.mode.label;</label>
                     <menulist id="pmcommander-cleartype-mode-menu" preference="pmcommander-pref-cleartype-mode" sizetopopup="always">
                        <menupopup>
                           <menuitem label="&pmcommander.graphics.cleartype.mode.auto;"         value="-1" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.default;"      value="0" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.aliased;"      value="1" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.gdiclassic;"   value="2" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.gdinatural;"   value="3" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.natural;"      value="4" />
                           <menuitem label="&pmcommander.graphics.cleartype.mode.symmetric;"    value="5" />
                        </menupopup>
                     </menulist>
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-cleartype-level-label" control="pmcommander-cleartype-level-input">&pmcommander.graphics.cleartype.level.label;</label>
                     <textbox type="number" size="3" min="-1" max="100" id="pmcommander-cleartype-level-input" preference="pmcommander-pref-cleartype-level" />
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-cleartype-contrast-label" control="pmcommander-cleartype-contrast-input">&pmcommander.graphics.cleartype.contrast.label;</label>
                     <textbox type="number" size="4" min="-1" max="1000" id="pmcommander-cleartype-contrast-input" preference="pmcommander-pref-cleartype-contrast" />
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-cleartype-gamma-label" control="pmcommander-cleartype-gamma-input">&pmcommander.graphics.cleartype.gamma.label;</label>
                     <textbox type="number" size="4" min="-1" max="2200" id="pmcommander-cleartype-gamma-input" preference="pmcommander-pref-cleartype-gamma" />
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-cleartype-pxmode-label" control="pmcommander-cleartype-pxmode-menu">&pmcommander.graphics.cleartype.pxmode.label;</label>
                     <menulist id="pmcommander-cleartype-pxmode-menu" preference="pmcommander-pref-cleartype-pixel-structure" sizetopopup="always">
                        <menupopup>
                           <menuitem label="&pmcommander.graphics.cleartype.pxmode.auto;" value="-1" />
                           <menuitem label="&pmcommander.graphics.cleartype.pxmode.flat;" value="0" />
                           <menuitem label="&pmcommander.graphics.cleartype.pxmode.RGB;"  value="1" />
                           <menuitem label="&pmcommander.graphics.cleartype.pxmode.BGR;"  value="2" />
                        </menupopup>
                     </menulist>
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-cleartype-gdimax-label" control="pmcommander-image-background-input">&pmcommander.graphics.cleartype.gdimaxsize;</label>
                     <textbox type="number" size="3" max="200" id="pmcommander-cleartype-gdimax-input" preference="pmcommander-pref-cleartype-gdi-max-size" />
                  </hbox>
               </groupbox>
            
            </tabpanel>
            <tabpanel id="pmcommander-graphics-color" orient="vertical">
               <checkbox id="pmcommander-graphics-color-override-a11y-check" preference="pmcommander-pref-color-override-a11y" label="&pmcommander.graphics.color.override.a11y;" />
               <groupbox id="pmcommander-graphics-color-ICC">
                  <caption label="&pmcommander.graphics.color.ICC.caption;" />
                  <label>&pmcommander.info.restartwarning;</label>
                  <checkbox id="pmcommander-color-ICCv4-check" preference="pmcommander-pref-color-ICCv4" label="&pmcommander.graphics.color.ICCv4.label;" />
                  <hbox align="center">
                     <label id="pmcommander-color-ICCmode-label" control="pmcommander-color-ICCmode-menu">&pmcommander.graphics.color.ICCmode.label;</label>
                     <menulist id="pmcommander-color-ICCmode-menu" preference="pmcommander-pref-color-ICCmode" sizetopopup="always">
                        <menupopup>
                           <menuitem label="&pmcommander.graphics.color.ICCmode.off;" value="0" />
                           <menuitem label="&pmcommander.graphics.color.ICCmode.always;" value="1" />
                           <menuitem label="&pmcommander.graphics.color.ICCmode.tagged;"  value="2" />
                        </menupopup>
                     </menulist>
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-color-ICCintent-label" control="pmcommander-color-ICCintent-menu">&pmcommander.graphics.color.ICCintent.label;</label>
                     <menulist id="pmcommander-color-ICCintent-menu" preference="pmcommander-pref-color-ICCintent" sizetopopup="always">
                        <menupopup>
                           <menuitem label="&pmcommander.graphics.color.ICCintent.embedded;" value="-1" />
                           <menuitem label="&pmcommander.graphics.color.ICCintent.perceptual;" value="0" />
                           <menuitem label="&pmcommander.graphics.color.ICCintent.relative;"    value="1" />
                           <menuitem label="&pmcommander.graphics.color.ICCintent.saturation;"  value="2" />
                           <menuitem label="&pmcommander.graphics.color.ICCintent.absolute;"    value="3" />
                        </menupopup>
                     </menulist>
                  </hbox>
               </groupbox>
            </tabpanel>
         </tabpanels>
      </tabbox>
   </prefpane>

   <prefpane id="pmcommander-pane-network" label="&pmcommander.pane.network;">
      <preferences>
         <preference id="pmcommander-pref-net-buffercount"        name="network.buffer.cache.count"               type="int" />
         <preference id="pmcommander-pref-net-buffersize"         name="network.buffer.cache.size"                type="int" />
         <preference id="pmcommander-pref-net-spdy"               name="network.http.spdy.enabled"                type="bool" />
         <preference id="pmcommander-pref-net-disable-hsts"       name="network.stricttransportsecurity.enabled"  type="bool" inverted="true"/>
         
         <preference id="pmcommander-pref-dns-ipv6"               name="network.dns.disableIPv6"                  type="bool" inverted="true"/>
         <preference id="pmcommander-pref-dns-prefetch"           name="network.dns.disablePrefetch"              type="bool" inverted="true"/>
         <preference id="pmcommander-pref-dns-cache"              name="network.dnsCacheEntries"                  type="int" />
         <preference id="pmcommander-pref-dns-expire"             name="network.dnsCacheExpiration"               type="int" />
         <preference id="pmcommander-pref-dns-urlfixup"           name="browser.fixup.alternate.enabled"          type="bool" />
         <preference id="pmcommander-pref-dns-urlfixup-prefix"    name="browser.fixup.alternate.prefix"           type="string" />
         <preference id="pmcommander-pref-dns-urlfixup-suffix"    name="browser.fixup.alternate.suffix"           type="string" />
         

         <preference id="pmcommander-pref-http-conn-timeout"      name="network.http.connection-timeout"          type="int" />
         <preference id="pmcommander-pref-http-retry-timeout"     name="network.http.connection-retry-timeout"    type="int" />
         <preference id="pmcommander-pref-http-fastfallback"      name="network.http.fast-fallback-to-IPv4"       type="bool" />
         <preference id="pmcommander-pref-http-keepalive-timeout" name="network.http.keep-alive.timeout"          type="int" />
         <preference id="pmcommander-pref-http-maxconn"           name="network.http.max-connections"             type="int" />
         <preference id="pmcommander-pref-http-persistent-maxconn-proxy"      name="network.http.max-persistent-connections-per-proxy" type="int" />
         <preference id="pmcommander-pref-http-persistent-maxconn-server"     name="network.http.max-persistent-connections-per-server" type="int" />
         <preference id="pmcommander-pref-http-diagnostics"       name="network.http.diagnostics"                 type="bool" />
         
         <preference id="pmcommander-pref-pipelining"             name="network.http.pipelining"                  type="bool" />
         <preference id="pmcommander-pref-pipelining-ssl"         name="network.http.pipelining.ssl"              type="bool" />
         <preference id="pmcommander-pref-pipelining-proxy"       name="network.http.proxy.pipelining"            type="bool" />
         <preference id="pmcommander-pref-pipelining-aggressive"  name="network.http.pipelining.aggressive"       type="bool" />
         <preference id="pmcommander-pref-pipelining-maxrequest-optimistic"   name="network.http.pipelining.max-optimistic-requests" type="int" />
         <preference id="pmcommander-pref-pipelining-maxrequest"  name="network.http.pipelining.maxrequests"      type="int" />
         <preference id="pmcommander-pref-pipelining-maxsize"     name="network.http.pipelining.maxsize"          type="int" />
         <preference id="pmcommander-pref-pipelining-readtimeout" name="network.http.pipelining.read-timeout"     type="int" />
         <preference id="pmcommander-pref-pipelining-reschedule"  name="network.http.pipelining.reschedule-on-timeout" type="bool" />
         <preference id="pmcommander-pref-pipelining-reschedule-timeout"      name="network.http.pipelining.reschedule-timeout" type="int" />
                  
      </preferences>
      <tabbox id="pmcommander-tabbox-net" flex="1">
         <tabs id="pmcommander-tabs-net">
<!--           <tab id="pmcommander-tab-net-general" label="&pmcommander.tab.general;" /> -->
            <tab id="pmcommander-tab-net-dnslookups"  label="&pmcommander.tab.net.dnslookups;" />
            <tab id="pmcommander-tab-net-http"  label="&pmcommander.tab.net.http;" />
            <tab id="pmcommander-tab-net-pipelining"  label="&pmcommander.tab.net.pipelining;" />
            <tab id="pmcommander-tab-net-advanced"  label="&pmcommander.tab.net.advanced;" />
         </tabs>
         
         <tabpanels flex="1">
            <tabpanel id="pmcommander-net-dnslookups" orient="vertical">
               <checkbox id="pmcommander-dns-urlfixup-check" preference="pmcommander-pref-dns-urlfixup" label="&pmcommander.net.dns.urlfixup.label;" />
               <hbox align="center" class="indent">
                  <label id="pmcommander-net-urlfixup-prefix-label" control="pmcommander-net-dns-urlfixup-prefix-input">&pmcommander.net.dns.urlfixup.prefix.label;</label>
                  <textbox size="6" id="pmcommander-net-dns-urlfixup-prefix-input" preference="pmcommander-pref-dns-urlfixup-prefix" />
               </hbox>
               <hbox align="center" class="indent">
                  <label id="pmcommander-net-urlfixup-suffix-label" control="pmcommander-net-dns-urlfixup-suffix-input">&pmcommander.net.dns.urlfixup.suffix.label;</label>
                  <textbox size="6" id="pmcommander-net-dns-urlfixup-suffix-input" preference="pmcommander-pref-dns-urlfixup-suffix" />
               </hbox>
               <groupbox id="pmcommander-net-dns">
                  <caption label="&pmcommander.net.dns.caption;" />
                  <checkbox id="pmcommander-dns-ipv6-check" preference="pmcommander-pref-dns-ipv6" label="&pmcommander.net.dns.ipv6.label;" />
                  <checkbox id="pmcommander-dns-prefetch-check" preference="pmcommander-pref-dns-prefetch" label="&pmcommander.net.dns.prefetch.label;" />
                  <hbox align="center">
                     <label id="pmcommander-net-dns-cache-label" control="pmcommander-net-dns-cache-input">&pmcommander.net.dns.cache.label;</label>
                     <textbox type="number" size="4" min="20" id="pmcommander-net-dns-cache-input" preference="pmcommander-pref-dns-cache" />
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-net-dns-expire-label" control="pmcommander-net-dns-expire-input">&pmcommander.net.dns.expire.label;</label>
                     <textbox type="number" size="5" min="60" max="65535" id="pmcommander-net-dns-expire-input" preference="pmcommander-pref-dns-expire" />
                     <label>&pmcommander.unit.seconds;</label>
                  </hbox>
               </groupbox>
            </tabpanel>
            <tabpanel id="pmcommander-net-http" orient="vertical">
               <checkbox id="pmcommander-net-http-fastfallback-check" preference="pmcommander-pref-http-fastfallback" label="&pmcommander.net.http.fastfallback.label;" />
               <groupbox id="pmcommander-net-http-timing">
                  <caption label="&pmcommander.net.http.timing.caption;" />
                  <hbox align="center">
                     <label id="pmcommander-net-http-conn-timeout-label" control="pmcommander-net-http-conn-timeout-input">&pmcommander.net.http.conn.timeout.label;</label>
                     <textbox type="number" size="3" min="10" max="600" id="pmcommander-net-http-conn-timeout-input" preference="pmcommander-pref-http-conn-timeout" />
                     <label>&pmcommander.unit.seconds;</label>
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-net-http-retry-timeout-label" control="pmcommander-net-http-retry-timeout-input">&pmcommander.net.http.retry.timeout.label;</label>
                     <textbox type="number" size="4" min="30" max="5000" id="pmcommander-net-http-retry-timeout-input" preference="pmcommander-pref-http-retry-timeout" />
                     <label>&pmcommander.unit.ms;</label>
                  </hbox>
                  <hbox align="center">
                     <label id="pmcommander-net-http-keepalive-timeout-label" control="pmcommander-net-http-keepalive-timeout-input">&pmcommander.net.http.keepalive.timeout.label;</label>
                     <textbox type="number" size="3" min="10" max="600" id="pmcommander-net-http-keepalive-timeout-input" preference="pmcommander-pref-http-keepalive-timeout" />
                     <label>&pmcommander.unit.seconds;</label>
                  </hbox>
               </groupbox>
               <groupbox id="pmcommander-net-http-connections">
